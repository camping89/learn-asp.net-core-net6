@if (TempData["Success"] is not null)
{
    <script type="text/javascript">
        toastr.success('@TempData["Success"]', '@TempData["Success"]')
    </script>
}

@if (TempData["Error"] is not null)
{
    <script type="text/javascript">
        toastr.error('@TempData["Error"]', '@TempData["Error"]')
    </script>
}

@if (TempData["Warning"] is not null)
{
    <script type="text/javascript">
        toastr.warning('@TempData["Warning"]', '@TempData["Warning"]')
    </script>
}